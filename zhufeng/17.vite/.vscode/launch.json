{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "vue-cli",
      "cwd": "${workspaceFolder}/packages/vite-project",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "port": 9229,
      "autoAttachChildProcesses": true,
      "stopOnEntry": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
